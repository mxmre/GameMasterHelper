<Page x:Class="GameMasterHelper.Pages.EditorPages"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:GameMasterHelper.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="EditorPages">

    <Grid x:Name="MainGrid">
        <TabControl x:Name="EditorInfo">
            <TabItem x:Name="EditorViewTab" Header="Информация">
                <Frame x:Name="EditorViewTabFrame" NavigationUIVisibility="Hidden"/>
            </TabItem>
            <TabItem x:Name="EditorEditTab" Header="Редактор">
                <Frame x:Name="EditorEditTabFrame" NavigationUIVisibility="Hidden"/>
            </TabItem>
        </TabControl>
    </Grid>
</Page>
